{
  "name": "Triage Helper",
  "version": "1.0.5",
  "manifest_version": 2,
  "description": "Making sure triagers take the right descision on an event",
 
  "icons": {
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_title": "Triage Helper",
    "default_popup": "main.html"
  },

  "background": {
    "scripts": ["js/background.js"]
  },

  "permissions": [
    "activeTab",
    "webNavigation",
    "tabs",
    "http://*/*",
    "https://*/*",
    "<all_urls>",
    "*://*/*",
    "identity",
    "*://*.google.com/*",
    "storage",
    "https://www.gmail.com/",
    "http://www.google.com/",
    "https://www.google.com/",
    "https://ssl.gstatic.com/",
    "https://www.googleapis.com/",
    "https://accounts.google.com/",
    "https://script.google.com/a/google.com/macros/s/AKfycbxKluhDx9PC-iaAfCUd9RCv0kbRxW6NZVNE6J9XQ-2_42Hly8Q/exec?*"
    ]
}
